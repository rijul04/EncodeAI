#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/create-agent/dist/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/create-agent/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/create-agent/dist/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/create-agent/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules/@daydreamsai/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/@daydreamsai+create-agent@0.3.0/node_modules:/home/ri1jgo0l/projects/EncodeAI/backendTest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@daydreamsai/create-agent/dist/index.js" "$@"
else
  exec node  "$basedir/../@daydreamsai/create-agent/dist/index.js" "$@"
fi
